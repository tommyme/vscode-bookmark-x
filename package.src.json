{
	"name": "bookmarkx",
	"displayName": "bookmark X",
	"description": "Drag-and-drop, grouping, sorting bookmarklet plugin",
	"version": "0.2.16",
	"publisher": "tommyme",
	"icon": "resources/icon.jpg",
	"homepage": "https://github.com/tommyme/vscode_bookmark_x",
	"repository": {
		"type": "git",
		"url": "https://github.com/tommyme/vscode_bookmark_x"
	},
	"engines": {
		"vscode": "^1.73.0"
	},
	"license": "SEE LICENSE IN LICENSE",
	"keywords": [
		"bookmark",
		"source",
		"productivity",
		"coding"
	],
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"vscode:package": "vsce package",
		"compile": "rollup -c --forceExit",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@eslint/js": "^9.9.0",
		"@rollup/plugin-commonjs": "^26.0.1",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@rollup/plugin-terser": "^0.4.4",
		"@rollup/plugin-typescript": "^11.1.6",
		"@types/eslint__js": "^8.42.3",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"@types/vscode": "^1.73.0",
		"eslint": "^9.9.0",
		"glob": "^7.1.7",
		"jsonc-parser": "^3.3.1",
		"lodash": "^4.17.21",
		"markdown-it": "^14.1.0",
		"prettier": "^3.3.3",
		"mocha": "^8.4.0",
		"semver": "^7.6.3",
		"tslib": "^2.6.3",
		"typescript": "^5.5.4",
		"typescript-eslint": "^8.2.0",
		"vscode-test": "^1.5.2",
		"rollup": "^4.19.1"
	},
	"dependencies": {}
}