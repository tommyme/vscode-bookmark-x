{
  "name": "book-mark-demo",
	"displayName": "book-mark-demo",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.59.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "book-mark-demo.toggleBookmark",
				"title": "book-mark-demo: toggle bookmark"
			},
			{
				"command": "book-mark-demo.addGroup",
				"title": "book-mark-demo: add group"
			}
		],
		"viewsContainers": {
			"activitybar": [{
				"id": "bookmarks",
				"title": "Bookmarks",
				"icon": "$(bookmark)"
			}]
		},
		"views": {
			"bookmarks": [{
					"id": "bookmarksByFile",
					"name": "By File",
					"icon": "$(bookmark)"
				},
				{
					"id": "bookmarksByGroup",
					"name": "By Group",
					"icon": "$(bookmark)"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.59.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	}
}
